<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StudentApp.College">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <Button Text="Google Drive" HeightRequest="120" WidthRequest="50" BackgroundColor="LightBlue" Clicked="Button_Clicked"/>
            </StackLayout>

            <StackLayout>
                <Button Text="Calendar" HeightRequest="120" WidthRequest="50" BackgroundColor="LightBlue" Clicked="Button_Clicked_1"/>
            </StackLayout>

            <StackLayout>
                <Button Text="Email" HeightRequest="120" WidthRequest="50" BackgroundColor="LightBlue" Clicked="Button_Clicked_2"/>
            </StackLayout>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>